Sub CopasFr()
    Windows("Master Data Process Quality.xlsm").Activate
    Sheets("Utama").Select
    Range("C2:C3").Select
    Selection.Copy
End Sub
Sub PasTo()
    Sheets("Daily 2025").Select
    Range("G1").Select
    PasValTrans
End Sub
Sub CopasFrTo()
    PasTo
    CopasDaily
End Sub
